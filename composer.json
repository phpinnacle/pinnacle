{
    "name": "phpinnacle/pinnacle",
    "type": "project",
    "description": "PHPinnacle Framework",
    "keywords": [
        "phpinnacle", "async", "framework"
    ],
    "homepage": "https://github.com/phpinnacle/pinnacle",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPinnacle",
            "email": "dev@phpinnacle.com",
            "homepage": "https://phpinnacle.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "amphp/log": "dev-master",
        "phpinnacle/ensign": "^1.0",
        "phpinnacle/identity": "^0.1",
        "phpinnacle/ridge": "dev-performance",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "symfony/dotenv": "^4.0"
    },
    "require-dev": {
        "amphp/file": "^0.3.3",
        "amphp/redis": "^0.3.3",
        "nikic/php-parser": "^4.0",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "PHPinnacle\\Pinnacle\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PHPinnacle\\Pinnacle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
